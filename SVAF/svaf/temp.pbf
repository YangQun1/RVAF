name:	"Compute 3D pointcload via Eadp Match"
version:	"v_1.08"
pause:	0
layer{
	name:	"data"
	type:	IMAGE_PAIR_FOLDER
	bottom:	"data"
	top:	"rectify"
	data_param{
		color: true
	}
	pairfolder_param{
		pair{
			left:	"F:/data/BaseData/left/"
			right:	"F:/data/BaseData/right/"
		}
	}
}
layer{
	name:	"rectify"
	type:	RECTIFY
	top:	"adaboost"
	rectify_param{
		filename:	"F:/svaf/data/calib/stereo_rectify_161103.dat"
	}
}
layer{
	name:	"adaboost"
	type:	ADABOOST
	top:	"eadp"
	adaboost_param{
		detector:	"F:/svaf/data/boost/detector_workpiece_161103.dat"
		sync_frame:	true
		#sync_video:	true
		epipolar:	true
		thresh:		40
		nms:		0.3
		pad_rect{
			left:	10
			right:	10
			top:	10
			bottom:	10
			xshift:	0
		}
	}
	
	display: true
}
layer{
	name:	"eadp"
	type:	EADP_MATCH
	top:	"3dpoint"
	display: true
	eadp_param{
		max_disp:	30
		factor:		2048
	}
}
layer{
	name:	"3dpoint"
	type:	TRIANG
	top:	"3dpoint"
	triang_param{
		visible:	false
		savepc:		true
		toolbox_dir:	"F:/svaf/calib/"
		calibmat_dir:	"F:/svaf/data/calib/mat_161103/"
	}
	
	display: true
}

